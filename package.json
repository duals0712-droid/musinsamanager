{
  "name": "moonager",
  "version": "0.1.0",
  "private": true,
  "main": "main.ts",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5180 TS_NODE_PROJECT=tsconfig.electron.json electronmon -r ts-node/register/transpile-only ./main.ts --watch main.ts --watch preload.ts",
    "build": "vite build && npm run build:main",
    "build:main": "tsc -p tsconfig.electron.json",
    "lint": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "lucide-react": "^0.308.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^27.1.0",
    "electronmon": "^2.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
